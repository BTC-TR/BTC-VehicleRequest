<mvc:View controllerName="com.btc.arachavuztalep.controller.Main" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc">
	<Page id="masterPage" title="Araç Havuz" showNavButton="true" navButtonPress="onNavBack" showHeader="false" enableScrolling="false">
		<content>
			<TileContainer id="container">
				<tiles>
					<StandardTile icon="sap-icon://add-activity" title="Talep Formu Oluştur" press="onTalepFormu"/>
					<StandardTile visible="false" icon="sap-icon://approvals" title="Talep Onay" press="onTalepOnay"/>
					<StandardTile icon="sap-icon://approvals" title="Taleplerim" press="onTaleplerim"/>
					<!--<StandardTile icon="sap-icon://approvals" title="Taleplerim2" press="onTaleplerim2"/>-->
					<StandardTile visible="false" icon="sap-icon://approvals" title="Fotoğraf Yükleme" press="onFotografYukle"/>
				</tiles>
			</TileContainer>
		</content>
		<footer></footer>
	</Page>
</mvc:View>